struct CRGB {
  unsigned char b; 
  unsigned char r; 
  unsigned char g; 
};

struct CRGB *leds;

#define PIN 4

void setup()
{
  FastSPI_LED.setLeds(NUM_LEDS);
  FastSPI_LED.setChipset(CFastSPI_LED::SPI_LPD8806);
  FastSPI_LED.setDataRate(0);
  FastSPI_LED.setPin(PIN);
  FastSPI_LED.init();
  FastSPI_LED.start();

  leds = (struct CRGB*)FastSPI_LED.getRGBData(); 
}

void loop() { 


  for(int j = 1 ; j < NUM_LEDS; j++ ) {
    for(int i = 0 ; i < NUM_LEDS; i++ ) {
      //memset(leds, 0, NUM_LEDS*3);
      leds[i].r = Rline[i+(j*NUM_LEDS)];
      leds[i].g = Gline[i+(j*NUM_LEDS)];
      leds[i].b = Bline[i+(j*NUM_LEDS)];

    }
  FastSPI_LED.show();
  //delay(1);
  }

}






